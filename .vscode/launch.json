{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Live Debug: W3/p2.ts",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "tsx",
        "watch",
        "--inspect",
        "W3/p3.ts"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "restart": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true
    }
  ]
}
